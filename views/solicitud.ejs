                            const fe = new Date(f.setDate(f.getDate() + 16)).toISOString('es-CO').split('T')[0];
                            this.rango = { start: this.rango.start, end: fe };
                            //Añade un día si el rango finaliza en domingo o inicia un fin de semana
                            if (this.rango.start.getDay() == 6 || this.rango.start.getDay() == 0 || fe.getDay() == 0) {
                                const fe2 = new Date(f.setDate(f.getDate() + 17)).toISOString('es-CO').split('T')[0];
                                this.rango = { start: this.rango.start, end: fe2 };
                            }
                        }
                        return this.rango.start + ' - ' + this.rango.end;
