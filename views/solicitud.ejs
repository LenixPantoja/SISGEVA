                suspendItem(item) {
                    this.solicitud.id_solicitud = Object.assign({}, item).id;
                    this.solicitud.id_empleado = Object.assign({}, item).id_emp;
                    let f1 = new Date(Object.assign({}, item).fecha_vac.substring(0, 10));
                    let fecha_vac_ini = new Date(f1.setDate(f1.getDate() + 1));
                    let f2 = new Date(Object.assign({}, item).fecha_vac.substring(13, 23));
                    let fecha_vac_fin = new Date(f2.setDate(f2.getDate() + 1));
                    let fecha_actual = new Date();
                    let dias = Object.assign({}, item).fecha_vac.substring(25, 26);

                    if (fecha_vac_ini.toISOString('es-CO').split('T')[0] > fecha_actual.toISOString('es-CO').split('T')[0] || fecha_vac_ini.toISOString('es-CO').split('T')[0] == fecha_actual.toISOString('es-CO').split('T')[0]) {
                        this.solicitud.dias_solicitados = dias;
                        this.suspend_di = true;
                    }
                    else if (fecha_actual.toISOString('es-CO').split('T')[0] == fecha_vac_fin.toISOString('es-CO').split('T')[0]) {
                        this.solicitud.dias_solicitados = 1;
                        this.suspend_di = true;
                    }
                    else if (fecha_actual.toISOString('es-CO').split('T')[0] > fecha_vac_fin.toISOString('es-CO').split('T')[0]) {
                        alert('La solicitud no puede ser suspendida, la fecha actual excede por mucho la fecha de vacaciones');
                    }
                    else {
                        this.solicitud.dias_solicitados = (fecha_vac_fin.getDate() - fecha_actual.getDate()) + 1;
                        this.suspend_di = true;
                    }
