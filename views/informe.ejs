    <link rel=" stylesheet" href="../node_modules/css/navegador.css">
    <link rel="stylesheet"  href="../css/style.css" />
</head>

<body>





    <!--Declaramos un div, este va abarcar todo los componenetes de vuetify, ademas en este se declaro un ID para luego llamarlo en vuetify -->
    <div id="solicitud">
        <v-app id="inspire" style="background: rgba(151, 162, 196, 0.146);">
            <div>
                <!-- Nav -->
                <v-app-bar-nav-icon @click='drawer= !drawer'></v-app-bar-nav-icon>
                    <div style="position: absolute; top: 0; right: 0;">
                    <v-btn  icon id="fullScreen" @click="toggleFullScreen()">
                        <v-icon size="21"
                            style="box-shadow: 0 0 5px #2f4978; color:#606F8C; background-color: white; border-radius: 100%; padding: 10px; font-weight: bold;">
                            {{icono_full_ex}}</v-icon>
                    </v-btn>
                    <v-btn  color="#FF0000" icon id="logout" href="logout">
                        <v-icon size="21"
                            style=" box-shadow: 0 0 5px #2f4978; color:#606F8C; background-color: white; border-radius: 100%; padding: 8px; font-weight: bold;">
                            mdi-close</v-icon>
                    </v-btn>
                
            </div>
                    
                    
     
                    
                    
                    <v-sheet color="#606F8C" height="125px" max-width="100%"
                        style="background: linear-gradient(to bottom right, #2f4978, #3c4084);">
                        
                        <v-card-text>
                            
                            <v-row justify="end" style="padding-right: 23px; padding-top: 21px; padding-left: 28px;">
                                <img src="/resources/2.png" style="margin-top: -4px;">
                                <img src="/resources/sisgeva3.png" style="margin-top: -4px;">
                                <v-spacer></v-spacer>
                                
                                
                            </v-row>
                        </v-card-text>
                    </v-sheet>

                    <template>
                        <link rel="stylesheet"  href="../css/style.css" />
                        
                        <v-progress-linear color="#5c76a8" value="0" rounded
                            style="height: 7px; margin-bottom: -50px; margin-top: -3px;"></v-progress-linear>
                        <v-row dense>
                            

                            

                            <v-card class="mx-auto" max-width="260" outlined
                                style="border-radius: 20px;  box-shadow: 0 0 5px rgba(3, 33, 97, 0.2);">
                                <v-list-item three-line>
                                    <v-list-item-content>
                                        <v-list-item-title class="text-h5 mb-1">
                                            <h4>Aprobadas</h4>
                                            <v-divider></v-divider>
                                        </v-list-item-title>
                                        <v-list-item-subtitle>
                                            <h3>Totales: {{cant_solis_apr_sis}}</h3>
                                        </v-list-item-subtitle>
                                        
                                    </v-list-item-content>
                                    <v-list-item-avatar size="60"
                                        style="box-shadow: 0 0 5px rgba(3, 33, 97, 0.2); background: linear-gradient(to bottom right, #58f058, #1a8d03); padding: 20px; margin-top: 20px;">
                                        <v-icon dark size="30">mdi-email-check</v-icon>
                                    </v-list-item-avatar>
                                </v-list-item>
                            </v-card>

                            <v-card class="mx-auto" max-width="280" outlined
                                style="border-radius: 20px;  box-shadow: 0 0 5px rgba(3, 33, 97, 0.2);">
                                <v-list-item three-line>
                                    <v-list-item-content>
                                        <v-list-item-title class="text-h5 mb-1">
                                            <h4>Suspendidas</h4>
                                            <v-divider></v-divider>
                                        </v-list-item-title>
                                        <v-list-item-subtitle>
                                            <h3>Totales: {{cant_solis_sus_sis}}</h3>
                                        </v-list-item-subtitle>
                                        
                                    </v-list-item-content>
                                    <v-list-item-avatar size="60"
                                        style="box-shadow: 0 0 5px rgba(3, 33, 97, 0.2); background: linear-gradient(to bottom right, #9a58f0, #8d0382); padding: 20px; margin-top: 20px;">
                                        <v-icon dark size="30">mdi-email-lock</v-icon>
                                    </v-list-item-avatar>
                                </v-list-item>
                            </v-card>

                            <v-card class="mx-auto" max-width="265" outlined
                                style="border-radius: 20px;  box-shadow: 0 0 5px rgba(3, 33, 97, 0.2);">
                                <v-list-item three-line>

                                    <v-list-item-content>
                                        <v-list-item-title class="text-h5 mb-1">
                                            <h4>Rechazadas</h4>
                                            <v-divider></v-divider>
                                        </v-list-item-title>
                                        <v-list-item-subtitle>
                                            <h3>Totales: {{cant_solis_rec_sis}}</h3>
                                        </v-list-item-subtitle>
                                        
                                    </v-list-item-content>
                                            
                                    <v-list-item-avatar size="60"
                                        style=" box-shadow: 0 0 5px rgba(3, 33, 97, 0.2); background: linear-gradient(to bottom right, #ff833c, #ff3e6a); padding: 20px;">
                                        <v-icon dark size="30">mdi-email-remove</v-icon>
                                    </v-list-item-avatar>
                                </v-list-item>
                            </v-card>
                        </v-row>
                    </template>

                

                    <v-card id="card_t">
                        <v-card-content>
                            <v-card-title>
                                <h2 class="px-1">{{ listado }}</h2>
                                <v-spacer>
                                    </v-spacer>
                                <v-text-field v-model="search" append-icon="mdi-magnify" label="Buscar" rounded outlined
                                    hide-details class="col-3" style="max-width: 350px;">
                                </v-text-field>

                                <!--   Seleccion para poder dar la info de solicitudes  -->
                               
                                <v-btn-toggle rounded >
                                    <v-btn color="#94C2D6" @click="listado='Empleados';listado_soli();" id="tg_one"
                                        style="box-shadow: 0 0 5px rgba(3, 33, 97, 0.2);">
                                        <v-icon>mdi-account-multiple</v-icon>
                                    </v-btn>
                                </v-btn-toggle>
                                
                                <v-col cols="auto ">
                                    <v-btn-toggle shaped mandatory v-model="toggle_exclusive"
                                        style=" box-shadow: 0 0 5px rgba(3, 33, 97, 0.2);">
                                        <v-btn color="#0515B9" text @click="listado='Todas';listado_soli();">
                                            <v-icon>mdi-email-multiple</v-icon>
                                        </v-btn>                                        
                                        <v-btn color="#42FF00" text @click="listado='Aprobada';listado_soli();">
                                            <v-icon>mdi-email-check</v-icon>
                                        </v-btn>
                                        <v-btn color="#CD00FF" text @click="listado='Suspendida';listado_soli();">
                                            <v-icon>mdi-email-lock</v-icon>
                                        </v-btn>
                                        <v-btn color="red" text @click="listado='Rechazada';listado_soli();">
                                            <v-icon>mdi-email-remove</v-icon>
                                        </v-btn>
                                    </v-btn-toggle>
                                </v-col>                          

                                </v-card-title>

                            <v-card-content style="width: max-content">
                                <v-data-table :headers="headers" :items="desserts" :search="search">                                                                     

                                    
                                    <template v-slot:no-data>
                                        <h3>No existen datos</h3>
                                    </template>
                                    <template v-slot:item.cant_vac_dias="{item }">
                                        <strong> {{ item.cant_vac_dias }}</strong>
                                    </template>
                                    <template v-slot:no-results>
                                        <h3> Lo sentimos, no hay resultado para '{{ search }}'.</h3>
                                    </template>
                                </v-data-table>
                            </v-card-content>
                        </v-card-content>
                    </v-card>
                </v-main>
            
            
            
            </div>
        </v-app>

        <!-- aqui continua el navbar, este muestra una lista con las clases que tenemos actualmente -->
        <v-navigation-drawer v-model="drawer" absolute temporary style="font-family: Roboto,sans-serif;">
            <v-list dense nav>
                
                <v-list-item link href='/empleados'>
                    <!-- Nos redirecciona a la vista sedes -->
                    <v-list-item-icon>
                        <v-icon>mdi-account-multiple</v-icon> <!-- Hacemos uso de un icon -->
                    </v-list-item-icon>
                    <v-list-item-content>
                        <v-list-item-title> Gestión Empleados</v-list-item-title> <!-- Mandamos el nombre de la vista -->
                    </v-list-item-content>
                </v-list-item>
    
                <v-list-item link href='/informe'>
                    <!-- Nos redirecciona a la vista empleados -->
                    <v-list-item-icon>
                        <v-icon>mdi-account-multiple</v-icon> <!-- Hacemos uso de un icon -->
                    </v-list-item-icon>
                    <v-list-item-content>
                        <v-list-item-title> Informes</v-list-item-title> <!-- Mandamos el nombre de la vista -->
                    </v-list-item-content>
                </v-list-item>
    
                <v-list-item link href='/solicitud'>
                    <!-- Nos redirecciona a la vista solicitud -->
                    <v-list-item-icon>
                        <v-icon>mdi-folder</v-icon> <!-- Hacemos uso de un icon -->
                    </v-list-item-icon>
                    <v-list-item-content>
                        <v-list-item-title> Solicitudes</v-list-item-title> <!-- Mandamos el nombre de la vista -->
                    </v-list-item-content>
                </v-list-item>
            </v-list>
            </v-navegation-drawer>

    </div>
               
    
    

    <!-- Hacemos uso de los cdn -->
    <script src="https://cdn.jsdelivr.net/npm/vue@2.x/dist/vue.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/vuetify@2.x/dist/vuetify.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.20.0/axios.js"
        integrity="sha512-nqIFZC8560+CqHgXKez61MI0f9XSTKLkm0zFVm/99Wt0jSTZ7yeeYwbzyl0SGn/s8Mulbdw+ScCG41hmO2+FKw=="
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10.0.2/dist/sweetalert2.all.min.js"></script>

    <!-- 1. Link Vue Javascript -->
    <script src='https://unpkg.com/vue/dist/vue.js'></script>

    <!-- 2. Link VCalendar Javascript (Plugin automatically installed) -->
    <script src='https://unpkg.com/v-calendar'></script>

    <script>
        // Solicitudes.
        let url = 'http://localhost:3000/api/solicitud/';
        // Información del usuario en sesión
        let url15 = 'http://localhost:3000/api/datos/';
        //Información de reporte
        let url2 = 'http://localhost:3000/api/reporte/';
        // Información  Empleados
        let url6 = 'http://localhost:3000/api/empleados/';  
        // Listado empleados
        let url3 = 'http://localhost:3000/api/empleado/';
        // Cantidad solicitudes actuales del sistema segùn su estado.
        let url4 = 'http://localhost:3000/api/cant_solicitudes_act_sis/';
        // Cantidad solicitudes del sistema segùn su estado.
        let url5 = 'http://localhost:3000/api/cant_solicitudes_sis/';        
        // Comprueba que el rango de fechas de la solicitud sea el idoneo considerando los requerimientos.
        let url7 = 'http://localhost:3000/api/efecha/';
        
        // Actualiza la solicitud.
        let url10 = 'http://localhost:3000/api/actualizar_soli/';
         
        
        
        
        

        //Creación del proyecto de Vue
        new Vue({
            el: '#solicitud',
            //Creación de Vuetify
            vuetify: new Vuetify(),
            data() {
                // Constante que modela el listado de los días feriados
                const feriados_f = [
                    {
                        description: 'prueba',
                        isComplete: false,
                        dates: [{ months: 3, days: 6 }],
                        color: 'red'
                    },
                ];
                return {
                    //inicializamos esta funcion en false para que cuando se haga clic en el navbar este se pueda desplegar
                    drawer: false,
                    //Datos usuario en sesión
                    rol_s: null,
                    area_s: null,                   
                    enabled: false,
                    toggle_exclusive: 0,
                    
                    search: '',
                    listado: 'Informe de Solicitudes y Empleados',                    
                    valid: true,                
                    // Obliga al usuario a seleccionar el tipo de la solicitud antes de seleccionar la fecha
                    
                    // Icono para el boton de pantalla completa
                    icono_full_ex: 'mdi-fullscreen',
                    //Datos Tabla
                    headers: [{}],
                    headers0: [ // Todas
                    { text: 'Año', value: 'año' },
                    { text: 'Mes', value: 'mes' },
                    { text: 'Estado Solicitud', value: 'estado' },
                    { text: 'Tipo Solicitud', value: 'tipo' },
                    { text: 'Aréa', value: 'area' },
                    { text: 'Cantidad', value: 'cantidad' }],

                    headers1: [ // Aprobadas
                    { text: 'Año', value: 'año' },
                    { text: 'Mes', value: 'mes' },
                    { text: 'Tipo Solicitud', value: 'tipo' },
                    { text: 'Aréa', value: 'area' },
                    { text: 'Cantidad', value: 'cantidad' }],

                    headers2: [ // Suspendidas
                    { text: 'Fecha', value: 'año' },
                    { text: 'Mes', value: 'mes' },
                    { text: 'Tipo Solicitud', value: 'tipo' },
                    { text: 'Aréa', value: 'area' },                    
                    { text: 'Sede', value: 'sede' },
                    { text: 'Cantidad', value: 'cantidad' }],

                    headers3: [ // Rechazadas
                    { text: 'Año', value: 'año' },
                    { text: 'Mes', value: 'mes' },
                    { text: 'Tipo Solicitud', value: 'tipo' },
                    { text: 'Aréa', value: 'area' },                    
                    { text: 'Sede', value: 'sede' },                   
                    { text: 'Cantidad', value: 'cantidad' }],
                    headers5: [ // Empleados
                    {text:'Cédula', value:'idEmpleado'},
                    {text:'Nombre', value:'nombre'},
                    {text:'Cargo', value:'cargoEmpleado'},
                    {text:'Area', value:'areaEmpleado'},
                    {text:'Sueldo', value:'sueldoEmpleado'},
                    {text:'Fecha Corte', value:'fechaCorteEmpleado'},
                    {text:'Fecha Suscripcion', value:'fechaSuscripcionEmpleado'},
                    {text:'Días Pendientes', value:'diasPendientesEmpleado'},
                    {text:'Días Disfrutados', value:'diasDisfrutadosEmpleado'},
                    {text:'Provisión', value:'provisionEmpleado'}],                
                    
                    desserts: [
                        {   
                            año: null,
                            mes: null,
                            tipo: null,
                            sede: null,
                            area: null,
                            cargo: null,
                            estado: null,
                            
                            idEmpleado: null,
                            nombreEmpleado: null,
                            cargoEmpleado: null,
                            areaEmpleado: null,
                            sueldoEmpleado: null,
                            fechaCorteEmpleado: null,
                            fechaSuscripcionEmpleado: null,
                            diasPendientesEmpleado: null,
                            diasDisfrutadosEmpleado: null,
                            provisionEmpleado: null,



                        },
                    ],  
                    
                    //Formato de fechas
                    modelConfig: {
                        type: 'string',
                        mask: 'YYYY-MM-DD'
                    },
                    rango: {
                        start: null,
                        end: null
                    },
                    rango2: {
                        start: null,
                        end: null
                    },
                    
                    // Cantidad solicitudes del sistema
                    
                    cant_solis_apr_sis: 0,
                    cant_solis_rec_sis: 0,                    
                    cant_solis_sus_sis: 0,
                    
                }

            },
            // Atributos del picker
            computed: {
                attributes() {
                    return [
                        // Attributes for feriados_f (feriados fijos)
                        ...this.feriados_f.map(feri => ({
                            dates: feri.dates,
                            dot: {
                                color: feri.color,
                                class: feri.isComplete ? 'opacity-75' : '',
                            },
                            popover: {
                                label: feri.description,
                            },
                            customData: feri,
                        })),
                    ];
                },
            },
            //Nos trae todos los datos de la API y nos las llena en cada variable
            created() {
                this.feriados();
                this.datosUsuario();
            },
            methods: {
                // Extrae los datos del usuario en sesión para aplicar los filtros pertinentes.
                datosUsuario: function () {
                    axios.get(url15)
                        .then(response => {
                            let datos = response.data;
                            this.rol_s = datos[0].nom_rol;
                            if (this.rol_s == 'Admi') { this.area_s = 'Todas'; }
                            else if (this.rol_s == 'Súper Admi') { this.area_s = 'Administrativa'; }
                            else if (this.rol_s == 'Súper Admi Op') { this.area_s = 'Operativa'; }
                            this.listado_emp();
                            this.listado_soli();
                            this.cant_solis();
                        });
                },
                // Devuelve la cantidad de solicitudes aprobadas y rechazadas del sistema.
                cant_solis: function () {
                    axios.get(url5+this.rol_s)
                        .then(response => {
                            let datos = response.data;
                            for (var i in datos) {
                                let estado = datos[i].nom_estado;
                                if (estado == 'Aprobada') { this.cant_solis_apr_sis = datos[i].cantidad; }                               
                                else if (estado == 'Rechazada') { this.cant_solis_rec_sis = datos[i].cantidad; }
                                else if (estado == 'Suspendida') { this.cant_solis_sus_sis = datos[i].cantidad; }
                                
                            }
                        });
                },

                
                
                

                
                //Permite almacenar el el array de solitudes en el sistema
                listado_soli: function () {
                    this.desserts.splice(0);
                    this.cant_solis();
                    
                    if (this.listado == 'Aprobada') { this.headers = this.headers1; }
                    else if (this.listado == 'Suspendida') { this.headers = this.headers2; }
                    else if (this.listado == 'Rechazada') { this.headers = this.headers3; }
                    else if (this.listado=='Empleados'){
                        this.headers= this.headers5;
                        axios.get(url6)
                        .then(response =>{
                            let datos= response.data;
                            for(var i in datos){
                                this.desserts.push({
                                    idEmpleado: datos[i].id_empleado,
                                    nombre: datos[i].empleado,
                                    cargoEmpleado: datos[i].nom_cargo,
                                    areaEmpleado: datos[i].nom_area,
                                    fechaCorteEmpleado: 'Pendiente',
                                    fechaSuscripcionEmpleado: 'Pendiente',
                                    diasPendientesEmpleado: datos[i].dias_vac,
                                    diasDisfrutadosEmpleado: datos[i].dias_dis,
                                    sueldoEmpleado: 'Pendiente',
                                    bonoEmpleado: 'Pendiente',
                                    provisionEmpleado: 'Pendiente'
                                })
                            }
                        })
                    }
                    else {this.headers = this.headers0;}

                    
                    if (this.listado != 'Aprobada'||this.listado != 'Suspendida'||this.listado != 'Rechazada') {
                        axios.get(url2 + this.listado)
                            .then(response => {
                                let datos = response.data;
                                for (var i in datos) {
                                    this.desserts.push({

                                        año: datos[i].año,                                                                                
                                        mes: datos[i].mes,
                                        tipo: datos[i].tipo_solicitud,
                                        area: datos[i].nom_area,
                                        cantidad: datos[i].cantidad
                                        
                                    });
                                }
                            });
                    }
                    

                    else {
                        this.headers = this.headers0;
                        axios.get(url2 +this.listado)
                            .then(response => {
                                let datos = response.data;
                                for (var i in datos) {
                                    this.desserts.push({
                                        año: datos[i].año,                                                                                
                                        mes: datos[i].mes,
                                        estado: datos[i].estado_solicitud,
                                        tipo: datos[i].tipo_solicitud,
                                        area: datos[i].nom_area,
                                        cantidad: datos[i].cantidad
                                    });
                                }
                            });
                    }
                },
                
                
                
                    
                    
                
                resetInfoC: function () {
                    // Borra toda la informaciòn del dialogo create.
                    this.$refs.create.reset();
                    this.rango = { start: null, end: null };
                    this.rango.start = null;
                    this.rango.end = null;
                    this.create_di = false;
                },
                resetInfoI: function () {
                    // Borra toda la informaciòn del dialogo edit.
                    this.edit_di = false;
                    this.$refs.edit.reset();
                    this.rango = { start: null, end: null };
                    this.rango.start = null;
                    this.rango.end = null;

                },
                // Funcion para la pantalla completa
                toggleFullScreen: function () {
                    if (!document.fullscreenElement) {
                        document.documentElement.requestFullscreen();
                        this.icono_full_ex = 'mdi-fullscreen-exit';

                    } else {
                        if (document.exitFullscreen) {
                            document.exitFullscreen();
                            this.icono_full_ex = 'mdi-fullscreen';
                        }
                    }
                },
                
                buscarEmpleado: function (id) {
                    axios.get(url3 + '/' + id).then(response => {
                        let datos = response.data;
                        this.empleado.nom_cargo = datos[0].nom_cargo;
                        this.empleado.nom_sede = datos[0].nom_sede;

                        this.solicitud.peri_ini = new Date(datos[0].periodo_ini).toISOString('es-CO').split('T')[0];
                        this.solicitud.peri_fin = new Date(datos[0].periodo_fin_1).toISOString('es-CO').split('T')[0];

                        if (datos[0].dias_dis_periodo_1 >= this.solicitud.dias_solicitados) {
                            this.validateRango(this.rango.start, this.rango.end);
                        }
                        else {
                            alert('Los días seleccionados sobrepasan los días disponibles para el periodo.');
                            this.solicitud.dias_solicitados = datos[0].dias_dis_periodo_1;
                            this.$refs.rango_fecha.reset();
                            this.rango = { start: null, end: null };
                            this.rango.start = null;
                            this.rango.end = null;
                        }
                    });
                },
                validateRango: function (f1, f2) {
                    let cargo = this.empleado.nom_cargo;
                    let sede = this.empleado.nom_sede;
                    let f3 = f1;
                    let f4 = f2;
                    axios.get(url7 + cargo + '/' + sede + '/' + f1 + '/' + f2 + '/' + f3 + '/' + f4)
                        .then(response => {
                            let datos = response.data;
                            if (datos == '') { this.createItem(); }
                            else {
